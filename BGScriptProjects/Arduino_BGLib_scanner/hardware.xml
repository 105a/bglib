<?xml version="1.0" encoding="UTF-8" ?>

<hardware>
    <sleeposc enable="false" ppm="30" />
    <usb enable="true" endpoint="api" />
    <txpower power="15" bias="5" />
    <!-- UART 1, alternate 1:
        P0_5 = RX
        P0_4 = TX
        P0_3 = RTS
        P0_2 = CTS
    -->
    <usart channel="1" alternate="1" baud="38400" endpoint="api" flow="false" />
</hardware>
